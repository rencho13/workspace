import java.util.Scanner;



		public class Quiz {
			static int n = 7;
			static boolean b = false;
			static void f(int p, int m, int n, boolean b){
				for (int i = p; i >= 0 ; i--){
					if ((i >= 3) && b){
		           // code maquant 1
						System.out.print((i + 2) + " ");
					}
					else {
		            // code manquant 2
						System.out.print((i + 1) + " ");
					}
				}
				int k = m;
				while (k <= n){
					System.out.print(k + " ");
					k*=2;
				}
			}

			public static void main(String[] args) {
				f(n,n/2,2*n,!b);
			}
		}