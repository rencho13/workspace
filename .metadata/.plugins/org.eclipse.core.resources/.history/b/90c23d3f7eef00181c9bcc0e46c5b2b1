
public class Morpion {

	public static void main(String[] args) {
		
		char grid [] = { 
				'1','2','3',
				'4','5','6',
				'7','8','9'
		};
		char p1 = 'O';
		char p2 = 'X';
			
	for (int i = 0 ; i <= grid.length ; i*=3)  {
	
		System.out.println("[" + grid[i] + '|' + grid[i+1] + '|' + grid[i+2] + ']'  );
		
	}
	
}
}


