
public class Tab {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		String a = "MO";
		String essai = "essai";
		String test = "";
		for (int i = 1; i <= 3; ++i) {
			test = test + essai.charAt(6-2*i);
			test = essai.charAt(i) + test;
		}
		System.out.println(test);
} }
